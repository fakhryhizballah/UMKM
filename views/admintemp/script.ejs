<script>
    $(document).ready(function () {
        var url = window.location;
        var element = $('ul#sidebarnav a').filter(function () {
            return this.href === url;
        }).addClass('collapse').parent().parent().addClass('show');
        if (element.is("li") && element.children("a").length !== 0) {
            element.children("a").addClass("collapse");
        }
    });
</script>